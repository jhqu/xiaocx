      <image src="{{baseurl+item.imageUrl}}"></image>
      <text>{{item.name}}</text>
    </view>
  </view>
  <ab-title titel1="视频课程" titel2="资源下载" descrip="vip互联网时代+时代"> </ab-title>
  <view class="list_box">
    <view wx:for="{{classarr[2]}}" wx:key="{{item.id}}" class="list">
      <image src="{{baseurl+item.imageUrl}}"></image>
      <text>{{item.name}}</text>
    </view>
  </view>
</view>